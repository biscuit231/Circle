<div>
  <h1 class="signUpTitle">Sign Up</h1>
  <form class="col s4 signBox signup-form">
    <div class="row">
      <div class="input-field col s12">

        <input class="inputMargin" id="userName" type="text" class="validate">
        <label class="colourTxt" for="UserName">User Name</label>
      </div>
      <div class="input-field col s12">
        <input class="inputMargin" id="firstName" type="text" class="validate">
        <label class="colourTxt" for="FirstName">First Name</label>
      </div>
      <div class="input-field col s12">
        <input class="inputMargin" id="lastName" type="text" class="validate">
        <label class="colourTxt" for="LastName">Last Name</label>
      </div>
      <div class="input-field col s12">
        <input class="inputMargin" id="email" type="email" class="validate">
        <label class="colourTxt" for="Email">Email</label>
      </div>
      <div class="input-field col s12">
        <input class="inputMargin" id="password" type="password" class="validate">
        <label class="colourTxt" for="Password">Password</label>
      </div>
      <div class="input-field col s12">
        <button type="submit" class="waves-effect waves-light btn inputMargin submitBtn">Submit</button>
      </div>
    </div>
  </form>
</div>

<script src="/js/users.js"></script>